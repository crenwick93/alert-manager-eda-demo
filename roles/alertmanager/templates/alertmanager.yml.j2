route:
  receiver: eda_webhook
  repeat_interval: 1m
  group_wait: 15s
  group_interval: 15s

receivers:
  - name: eda_webhook
    webhook_configs:
      - url: '{{ eda_webhook_url }}'
        http_config:
          bearer_token: /etc/alertmanager/eda_token
          tls_config:
            insecure_skip_verify: true
